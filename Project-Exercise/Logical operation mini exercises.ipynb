   "source": [
    "# Logical Operations\n",
    "x, y, z = True, False, True\n",
    "\n",
    "print(\"x and y:\", x and y)\n",
    "print(\"x or y:\", x or y)\n",
    "print(\"not x:\", not x)\n",
    "print(\"(x and y) or z:\", (x and y) or z)\n",
    "print(\"x == z:\", x == z)\n",
    "print(\"x is z:\", x is z)  # identity check (True here since True is singleton)"
   ]
  }
